{
  "name": "timereg",
  "description": "Quick timeregistration app. To help remember what you did.",
  "version": "3.0.2",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.62",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "idb": "^5.0.7",
    "luxon": "^1.25.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux-devtools-extension": "^2.13.8",
    "tailwindcss": "^1.8.13",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "start": "parcel serve ./src/index.html",
    "build": "parcel build ./src/index.html --no-scope-hoist",
    "commit": "cz",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pwaManifest": {
    "name": "Timereg",
    "theme": "#1a202c",
    "display": "standalone",
    "generateIconOptions": {
      "baseIcon": "./src/public/clock.png",
      "sizes": [
        128,
        192,
        512
      ],
      "genFavicons": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/luxon": "^1.25.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@web/dev-server": "0.0.13",
    "commitizen": "^4.2.1",
    "husky": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-nightly.433",
    "parcel-config-pwa-manifest": "0.0.8",
    "postcss": "^8.1.4",
    "redux-devtools": "^3.7.0",
    "standard-version": "^9.0.0"
  }
}
