{
  "name": "timereg",
  "description": "Quick timeregistration app. To help remember what you did.",
  "version": "3.1.2",
  "private": true,
  "dependencies": {
    "idb": "^6.0.0"
  },
  "scripts": {
    "buildcss": "postcss src/Gronia.Timereg.Client/wwwroot/css/app.css -o src/Gronia.Timereg.Client/wwwroot/css/app.min.css",
    "build-indexed-db": "esbuild src/Gronia.Timereg.IndexedDb/wwwroot/indexedDb.js --bundle --minify --sourcemap --format=esm --outfile=src/Gronia.Timereg.IndexedDb/wwwroot/indexedDb.min.js"
  },
  "browserslist": {
    "production": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "autoprefixer": "^10.3.1",
    "esbuild": "^0.12.20",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.7"
  }
}
